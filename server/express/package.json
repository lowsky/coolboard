{
  "name": "express",
  "version": "1.0.0",
  "description": "express based server",
  "main": "src/index.js",
  "scripts": {
    "build": "babel --no-babelrc --presets @babel/env -d build src middleware",
    "start:prodbuild": "node -r dotenv/config build",
    "start:nonoptimized": "OPTIMIZED=false node -r dotenv/config build",
    "start": "nodemon -e js,graphql -x babel-node -r dotenv/config -b @babel/preset-env src",
    "debug": "nodemon -e js,graphql -x babel-node -r dotenv/config -b @babel/preset-env --inspect express",
    "start2": "nodemon -e js,graphql -x babel-node -b @babel/preset-env -r dotenv/config src",
    "debug2": "nodemon -e js,graphql -x babel-node -b @babel/preset-env --inspect src",
    "playground": "prisma playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=12.1"
  },
  "dependencies": {
    "@babel/core": "7.14.8",
    "apollo-errors": "1.9.0",
    "apollo-server-express": "2.25.2",
    "apollo-server-micro": "2.25.2",
    "aws-sdk": "2.957.0",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.3",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.4",
    "prisma-client-lib": "1.34.12",
    "utf-8-validate": "5.0.5",
    "webpack-node-externals": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/node": "7.14.7",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.14.8",
    "@graphql-tools/schema": "7.1.5",
    "@instana/collector": "1.129.0",
    "core-js": "3.16.0",
    "dotenv": "10.0.0",
    "express-jwt": "6.0.0",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5"
  }
}
