{
  "name": "trello-gateway",
  "version": "0.1.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "netlify-lambda build src/lambda",
    "start": "nodemon -e js,graphql -x babel-node -b @babel/preset-env src/express",
    "debug": "nodemon -e js,graphql -x babel-node -b @babel/preset-env --inspect src/express",
    "playground": "prisma playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=12.1"
  },
  "dependencies": {
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.14.2",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.1",
    "graphql": "15.0.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.8.0",
    "netlify-lambda": "1.6.3",
    "prisma-client-lib": "1.34.10",
    "utf-8-validate": "5.0.2"
  },
  "devDependencies": {
    "@babel/node": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@instana/collector": "1.100.1",
    "apollo-engine": "1.1.2",
    "dotenv": "8.2.0",
    "express-jwt": "5.3.3",
    "graphql-tools": "6.0.8",
    "graphql-yoga": "1.18.3",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5"
  }
}
