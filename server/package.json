{
  "name": "coolboardsecure",
  "version": "0.1.0",
  "description": "A GraphQL server based on Prisma",
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "netlify-lambda build src/lambda",
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "prisma playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "author": "Robert Hostlowsky",
  "private": "yes",
  "dependencies": {
    "apollo-errors": "1.9.0",
    "aws-lambda": "0.1.2",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.1",
    "graphql": "14.1.1",
    "graphql-yoga": "1.16.9",
    "jsonwebtoken": "8.5.1",
    "netlify-lambda": "1.1.1",
    "prisma-binding": "2.2.14",
    "utf-8-validate": "5.0.2"
  },
  "devDependencies": {
    "dotenv": "8.0.0",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "prisma": "1.23.4"
  }
}
