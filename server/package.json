{
  "name": "trello-gateway",
  "version": "0.2.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "yarn prisma generate && netlify-lambda build src/lambda",
    "playground": "prisma playground",
    "dev": "npm-run-all --parallel start:lambda playground"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=12.1"
  },
  "dependencies": {
    "@instana/aws-lambda": "1.119.2",
    "@prisma/client": "^2.23.0",
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.23.0",
    "apollo-server-micro": "2.23.0",
    "bcryptjs": "2.4.3",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.3",
    "netlify-lambda": "2.0.6",
    "prisma-client-lib": "1.34.12"
  },
  "devDependencies": {
    "bufferutil": "4.0.3",
    "netlify": "6.1.29",
    "netlify-cli": "3.29.15",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prisma": "2.23.0",
    "prisma1": "1.34.12",
    "utf-8-validate": "5.0.5"
  }
}
