{
  "name": "trello-gateway",
  "version": "0.1.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "netlify-lambda build src/lambda -c src/.webpack.config.js",
    "start": "nodemon -e js,graphql -x babel-node -b @babel/preset-env -r dotenv/config src/express",
    "debug": "nodemon -e js,graphql -x babel-node -b @babel/preset-env --inspect src/express",
    "playground": "prisma playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=12.1"
  },
  "dependencies": {
    "@instana/aws-lambda": "1.106.6",
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.19.0",
    "apollo-server-micro": "2.19.0",
    "aws-sdk": "2.784.0",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.2",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.11.0",
    "netlify-lambda": "2.0.1",
    "prisma-client-lib": "1.34.10",
    "utf-8-validate": "5.0.3",
    "webpack-node-externals": "2.5.2"
  },
  "devDependencies": {
    "@babel/node": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "apollo-engine": "1.1.2",
    "dotenv": "8.2.0",
    "express-jwt": "6.0.0",
    "graphql-tools": "7.0.1",
    "graphql-yoga": "1.18.3",
    "nodemon": "2.0.6",
    "npm-run-all": "4.1.5"
  }
}
