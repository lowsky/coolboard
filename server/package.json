{
  "name": "trello-gateway",
  "version": "0.2.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start": "yarn build && yarn netlify dev -p 3000",
    "studio": "prisma studio",
    "build": "prisma generate && yarn compile",
    "compile": "yarn babel src --out-dir functions"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@instana/aws-lambda": "1.119.2",
    "@prisma/client": "^2.23.0",
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.23.0",
    "apollo-server-micro": "2.23.0",
    "bcryptjs": "2.4.3",
    "graphql": "15.5.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "bufferutil": "4.0.3",
    "netlify": "6.1.29",
    "netlify-cli": "3.29.15",
    "nodemon": "2.0.7",
    "prisma": "2.23.0",
    "utf-8-validate": "5.0.5"
  }
}
