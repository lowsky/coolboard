{
  "name": "trello-clone-backend",
  "version": "1.0.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start": "yarn build && yarn netlify dev -p 3000",
    "studio": "prisma studio",
    "build": "prisma generate && yarn compile",
    "compile": "babel --verbose --delete-dir-on-start src --out-dir functions"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@instana/aws-lambda": "1.125.1",
    "@prisma/client": "2.25.0",
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.25.2",
    "apollo-server-micro": "2.25.2",
    "bcryptjs": "2.4.3",
    "graphql": "15.5.1",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "bufferutil": "4.0.3",
    "netlify": "7.0.1",
    "netlify-cli": "3.38.10",
    "prisma": "2.25.0",
    "utf-8-validate": "5.0.5"
  }
}
