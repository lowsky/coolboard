{
  "name": "trello-gateway",
  "version": "0.1.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "netlify-lambda build src/lambda",
    "start": "nodemon -e js,graphql -x node src/express/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect src/express/index.js",
    "playground": "prisma playground",
    "dev": "npm-run-all --parallel start playground"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=12.1"
  },
  "dependencies": {
    "@instana/collector": "1.95.2",
    "apollo-engine": "1.1.2",
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.11.0",
    "aws-lambda": "1.0.6",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.1",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-jwt": "5.3.3",
    "graphql": "15.0.0",
    "graphql-binding": "2.5.2",
    "graphql-import": "1.0.2",
    "graphql-tools": "5.0.0",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.8.0",
    "netlify-lambda": "1.6.3",
    "node-fetch": "2.6.0",
    "prisma-binding": "2.3.16",
    "utf-8-validate": "5.0.2"
  },
  "devDependencies": {
    "dotenv": "8.2.0",
    "nodemon": "2.0.3",
    "npm-run-all": "4.1.5",
    "prisma": "1.34.10"
  }
}
