{
  "ignoreDeps": ["@types/.*"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "masterIssue": true,
  "rebaseConflictedPrs": false,
  "rebaseStalePrs": false,
  "statusCheckVerify": true,
  "extends": ["config:base"]
}
