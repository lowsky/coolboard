{
  "name": "coolboard",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "homepage": "/",
  "dependencies": {
    "@apollo/client": "3.6.5",
    "@chakra-ui/icons": "2.0.0",
    "@chakra-ui/react": "2.1.0",
    "@clerk/clerk-sdk-node": "3.6.0",
    "@clerk/nextjs": "3.6.5",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@instana/aws-lambda": "2.3.0",
    "@instana/opentelemetry-exporter": "2.3.0",
    "@netlify/functions": "1.0.0",
    "@opentelemetry/api": "1.1.0",
    "@opentelemetry/core": "1.2.0",
    "@opentelemetry/instrumentation-express": "0.29.0",
    "@opentelemetry/instrumentation-graphql": "0.28.0",
    "@opentelemetry/instrumentation-http": "0.28.0",
    "@opentelemetry/node": "0.24.0",
    "@prisma/client": "3.14.0",
    "apollo-server-errors": "3.3.1",
    "apollo-server-lambda": "3.8.1",
    "apollo-server-micro": "3.7.0",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.6",
    "emitter-listener": "1.1.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "framer-motion": "6.3.3",
    "graphql": "16.5.0",
    "graphql-tag": "2.12.6",
    "inter-ui": "3.19.3",
    "micro": "9.3.4",
    "next": "12.1.6",
    "prisma": "3.14.0",
    "react": "18.1.0",
    "react-apollo-network-status": "5.1.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.1.0",
    "react-icons": "4.3.1",
    "react-is": "18.1.0",
    "react-timeago": "6.2.1",
    "utf-8-validate": "5.0.9"
  },
  "scripts": {
    "dev": "echo you wanna run:  yarn start  instead; yarn start",
    "start": "PRISMA_CLIENT_ENGINE_TYPE=dataproxy next dev",
    "start-production": "next start",
    "lint": "next lint",
    "trigger-e2e-smoke": "sh trigger-circleci-smoke.sh",
    "smoketest": "cd smoketest; yarn --immutable ; yarn cy open -C cypress-local.json",
    "build": "yarn run metainfo && next build",
    "prisma:generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "prisma:migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "metainfo": "sh storeMetaInfo.sh",
    "vercel-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build ; yarn lint; yarn trigger-e2e-smoke",
    "vercel-fast-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.1",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/zen-observable": "0.8.3",
    "@typescript-eslint/parser": "5.26.0",
    "apollo-server-core": "3.8.1",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.2.1"
}
