    "jwks-rsa": "2.1.1",
    "micro": "9.3.4",
    "next": "12.1.6",
    "prisma": "3.13.0",
    "react": "17.0.2",
    "react-apollo-network-status": "5.1.0",
    "react-dnd": "15.1.2",
    "react-dnd-html5-backend": "15.1.3",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-is": "17.0.2",
    "react-timeago": "6.2.1",
