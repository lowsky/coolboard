{
  "name": "coolboard",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "installConfig": {
    "pnp": false
  },
  "homepage": "/",
  "dependencies": {
    "@apollo/client": "3.3.21",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-link-ws": "1.0.20",
    "apollo-utilities": "1.3.4",
    "auth0-lock": "11.30.3",
    "fomantic-ui-css": "2.8.8",
    "graphql": "15.5.1",
    "graphql-tag": "2.12.5",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-apollo-network-status": "5.0.1",
    "react-dnd": "13.1.1",
    "react-dnd-html5-backend": "12.1.1",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-timeago": "6.2.1",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.0.3",
    "styled-components": "5.3.0",
    "subscriptions-transport-ws": "0.9.18"
  },
  "scripts": {
    "postinstall": "cd server ; yarn --frozen-lockfile",
    "start": "react-scripts start",
    "trigger-e2e-smoke": "sh trigger-circleci-smoke.sh",
    "build": "npm-run-all -p build:client build:server ",
    "vercel-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build:client ; yarn trigger-e2e-smoke",
    "build:client": "echo '{\"GIT_SHA1\" : \"'$COMMIT_REF'\" }' > public/meta.json && react-scripts build",
    "build:server": "cd server ; yarn run build"
  },
  "devDependencies": {
    "@types/react": "17.0.14",
    "http-proxy-middleware": "2.0.1",
    "npm-run-all": "4.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
