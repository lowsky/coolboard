{
  "name": "coolboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12.1"
  },
  "installConfig": {
    "pnp": false
  },
  "homepage": "/",
  "dependencies": {
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-link-ws": "1.0.20",
    "apollo-utilities": "1.3.4",
    "auth0-lock": "11.27.0",
    "fomantic-ui-css": "2.8.7",
    "graphql": "15.3.0",
    "graphql-tag": "2.11.0",
    "graphql-tools": "7.0.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-apollo": "3.1.5",
    "react-apollo-network-status": "5.0.0",
    "react-dnd": "11.1.3",
    "react-dnd-html5-backend": "11.1.3",
    "react-dom": "16.14.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.0",
    "react-timeago": "5.2.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.0.0",
    "styled-components": "5.2.0",
    "subscriptions-transport-ws": "0.9.18"
  },
  "scripts": {
    "postinstall": "cd server ; yarn --frozen-lockfile",
    "start": "react-scripts start",
    "build": "echo '{\"GIT_SHA1\" : \"'$COMMIT_REF'\" }' > public/meta.json && react-scripts build && npm run build:server",
    "build:server": "cd server ; npm run build"
  },
  "devDependencies": {
    "http-proxy-middleware": "1.0.6"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
