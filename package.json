{
  "name": "trello-gateway",
  "version": "0.2.0",
  "description": "A GraphQL Trello server based on Prisma",
  "scripts": {
    "start:lambda": "node -r dotenv/config node_modules/.bin/netlify-lambda serve src/lambda",
    "build": "prisma generate && yarn compile && echo not: netlify-lambda build src/lambda && echo not: yarn deploy-lib",
    "compile": "yarn babel src --out-dir functions",
    "deploy-lib": "cp -v ./node_modules/prisma/query-engine-rhel-openssl-1.0.x . && cp -v ./node_modules/prisma/query-engine-rhel-openssl-1.0.x lambda",
    "localdev": "yarn start:lambda"
  },
  "author": "Robert Hostlowsky",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@prisma/client": "2.23.0",
    "apollo-errors": "1.9.0",
    "apollo-server-lambda": "2.23.0",
    "apollo-server-micro": "2.23.0",
    "bcryptjs": "2.4.3",
    "encoding": "0.1.13",
    "graphql": "15.5.0",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.4",
    "bufferutil": "4.0.3",
    "netlify-cli": "3.31.16",
    "netlify-lambda": "2.0.7",
    "node-fetch": "^2.6.1",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prisma": "2.23.0",
    "utf-8-validate": "5.0.5"
  }
}
