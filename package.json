    "@opentelemetry/instrumentation-graphql": "0.32.0",
    "@opentelemetry/instrumentation-http": "0.33.0",
    "@opentelemetry/node": "0.24.0",
    "@prisma/client": "4.5.0",
    "apollo-server-errors": "3.3.1",
    "apollo-server-lambda": "3.11.1",
    "apollo-server-micro": "3.11.1",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.7",
    "emitter-listener": "1.1.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "framer-motion": "7.6.4",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "inter-ui": "3.19.3",
    "micro": "9.4.1",
    "next": "13.0.2",
    "prisma": "4.5.0",
    "react": "18.2.0",
    "react-apollo-network-status": "5.2.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0",
    "react-is": "18.2.0",
    "react-timeago": "7.1.0",
    "utf-8-validate": "5.0.10"
  },
  "scripts": {
    "dev": "echo you wanna run:  yarn start  instead; yarn start",
    "start": "PRISMA_CLIENT_ENGINE_TYPE=dataproxy next dev",
    "start-production": "next start",
    "lint": "next lint",
    "trigger-e2e-smoke": "sh trigger-circleci-smoke.sh",
    "smoketest": "cd smoketest; yarn --immutable ; yarn cy open --e2e -b electron",
    "build": "yarn run metainfo && yarn run prisma:generate && next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "DATABASE_URL=\"$MIGRATE_DATABASE_URL\" prisma migrate deploy",
    "metainfo": "sh storeMetaInfo.sh",
    "vercel-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build ; yarn lint; yarn trigger-e2e-smoke",
    "vercel-fast-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@graphql-codegen/typescript-react-apollo": "3.3.6",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/zen-observable": "0.8.3",
    "@typescript-eslint/parser": "5.42.0",
    "apollo-server-core": "3.11.1",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "packageManager": "yarn@3.2.4"
}
