{
  "name": "coolboard",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "installConfig": {
    "pnp": false
  },
  "homepage": "/",
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@prisma/client": "3.3.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-errors": "1.9.0",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-server-micro": "3.4.0",
    "auth0-lock": "11.31.0",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.5",
    "fomantic-ui": "2.8.8",
    "fomantic-ui-css": "2.8.8",
    "graphql": "15.7.2",
    "graphql-tag": "2.12.5",
    "inter-ui": "3.19.3",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.5",
    "lodash": "4.17.21",
    "micro": "9.3.4",
    "next": "11.1.2",
    "prisma": "3.3.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-apollo-network-status": "5.1.0",
    "react-dnd": "14.0.4",
    "react-dnd-html5-backend": "14.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-timeago": "6.2.1",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.0.4",
    "styled-components": "5.3.3",
    "utf-8-validate": "5.0.7"
  },
  "scripts": {
    "dev": "echo you wanna run:  yarn start  instead; yarn start",
    "start": "next dev",
    "start-production": "next start",
    "trigger-e2e-smoke": "sh trigger-circleci-smoke.sh",
    "fomantic": "gulp -f src/fomantic/gulpfile.js",
    "fomantic:help": "gulp -f src/fomantic/gulpfile.js -T",
    "fomantic:watch": "gulp -f src/fomantic/gulpfile.js watch",
    "fomantic:build": "gulp -f src/fomantic/gulpfile.js build",
    "build": "echo '{\"GIT_SHA1\" : \"'$COMMIT_REF'\" }' > public/meta.json && prisma generate && yarn fomantic:build && next build",
    "vercel-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build ; yarn trigger-e2e-smoke"
  },
  "devDependencies": {
    "@types/react": "17.0.33",
    "eslint": "8.1.0",
    "eslint-config-next": "11.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
