{
  "name": "coolboard",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "installConfig": {
    "pnp": false
  },
  "homepage": "/",
  "dependencies": {
    "@apollo/client": "3.5.8",
    "@auth0/nextjs-auth0": "1.7.0",
    "@instana/aws-lambda": "1.138.0",
    "@instana/opentelemetry-exporter": "1.137.5",
    "@netlify/functions": "0.11.0",
    "@opentelemetry/api": "1.1.0",
    "@opentelemetry/core": "1.0.1",
    "@opentelemetry/instrumentation-express": "0.27.1",
    "@opentelemetry/instrumentation-graphql": "0.27.2",
    "@opentelemetry/instrumentation-http": "0.27.0",
    "@opentelemetry/node": "0.24.0",
    "@prisma/client": "3.8.1",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-errors": "1.9.0",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-schema": "1.2.5",
    "apollo-server-lambda": "3.6.3",
    "apollo-server-micro": "3.6.3",
    "bcryptjs": "2.4.3",
    "bufferutil": "4.0.6",
    "emitter-listener": "1.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "fomantic-ui": "2.8.8",
    "fomantic-ui-css": "2.8.8",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "inter-ui": "3.19.3",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "2.0.5",
    "lodash": "4.17.21",
    "micro": "9.3.4",
    "next": "12.0.10",
    "prisma": "3.8.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-apollo-network-status": "5.1.0",
    "react-dnd": "14.0.5",
    "react-dnd-html5-backend": "14.1.0",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-timeago": "6.2.1",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "2.1.1",
    "styled-components": "5.3.3",
    "utf-8-validate": "5.0.8"
  },
  "scripts": {
    "dev": "echo you wanna run:  yarn start  instead; yarn start",
    "start": "next dev",
    "start-production": "next start",
    "lint": "next lint",
    "trigger-e2e-smoke": "sh trigger-circleci-smoke.sh",
    "fomantic": "gulp -f src/fomantic/gulpfile.js",
    "fomantic:help": "gulp -f src/fomantic/gulpfile.js -T",
    "fomantic:watch": "gulp -f src/fomantic/gulpfile.js watch",
    "fomantic:build": "echo temporary disabled: using pre-built files in src/fomantic/dist",
    "fomantic:build:manual": "gulp -f src/fomantic/gulpfile.js build",
    "build": "npm run metainfo && prisma generate && yarn fomantic:build && next build",
    "metainfo": "sh storeMetaInfo.sh",
    "vercel-build": "COMMIT_REF=$VERCEL_GITHUB_COMMIT_SHA yarn run build ; yarn lint; yarn trigger-e2e-smoke",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.1",
    "@graphql-codegen/typescript-operations": "2.3.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.5",
    "@types/graphql": "14.5.0",
    "@types/react": "17.0.37",
    "@types/react-dnd": "3.0.2",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.22",
    "@types/zen-observable": "0.8.3",
    "@typescript-eslint/parser": "5.11.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
